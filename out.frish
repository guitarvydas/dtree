if (found) {
    if (%funcall incompilingstate) {
        if (foundimmediate) {
            %funcall exec(item)
        } else {
            %funcall compileword(item)
        }
    } else {
        %funcall exec(item)
    }
} else {
    if (%funcall incompilingstate) {
        if (%funcall isinteger(item)) {
            %funcall compileinteger(item)
        } else {
            if (%funcall isfloat(item)) {
                %funcall compilefloat(item)
            } else {
                %funcall returnFalse
            }
        }
    } else {
        if (%funcall isinteger(item)) {
            %funcall pushasinteger(item)
        } else {
            if (%funcall isfloat(item)) {
                %funcall pushasfloat(item)
            } else {
                %funcall returnFalse
            }
        }
    }
}