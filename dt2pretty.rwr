% rewrite dt2pretty {
  Main [choice] = ‛«choice»’
  Choice [lb pred or1 yes colon1 yesbranch or2 no colon2 nobranch rb] = ‛\n«lb» «pred»⤷\n«or1»«yes»«colon1»⤷«yesbranch»⤶⤶⤷\n«or2»«no»«colon2»⤷«nobranch»⤶⤶\n«rb»⤶’
  Branch [x] = ‛\n«x»’
  Predicate [function_call] = ‛«function_call»’
  Action [function_call] =‛«function_call»’ 
  function_call [cs+] = ‛«cs»’
  char_newline [_] = ‛_’
  char_questionmark [_] = ‛?’
  char_percent [_] = ‛%’
  char_begindiv [_] = ‛’
  char_enddiv [_] = ‛’
  char_beginspan [lb cs* rb] = ‛’
  char_endspan [_] = ‛’
  char_at [_] = ‛@’
  char_space [_] = ‛’
  char_nonbreakingspace [_] = ‛’
  char_other [c] =‛«c»’
}
